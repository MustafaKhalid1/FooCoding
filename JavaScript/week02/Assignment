// 8: Write a program that checks the types of two variables and prints out SAME TYPE if they are the same type.
let int=10
console.log(Number)
let String='text'
console.log(String)
let Boolean= true
console.log(Boolean)
let Object=null
console.log(Object)
console.log(' The type of the int is: number')
console.log(' The type of the string is: text')
console.log(' The type of the boolean is: boolean value')
console.log(' The type of the object is: object')


console.log(typeof int)
console.log(typeof String)
console.log(typeof Boolean)
console.log(typeof Object)

if(int===String){
  console.log('same type')
}
else { console.log('different type')
     }
if(int===Boolean){
  console.log('same type');
}
else { console.log('different type')
     }
if(int===Object){
  console.log('same type');
}
else { console.log('different type')
     }
if(String===Boolean){
  console.log('same type')
}
else{
  console.log('different type')
}
if(String===Object){
  console.log('same typr')
}
else{
  console.log('different type')
}
if(Boolean===Object){
  console.log('same type')
}
else{
  console.log('different type')
}

